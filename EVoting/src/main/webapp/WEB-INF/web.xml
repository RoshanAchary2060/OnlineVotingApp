<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>RegistrationControllerServlet</servlet-name>
    <servlet-class>evoting.controller.RegistrationControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginControllerServlet</servlet-name>
    <servlet-class>evoting.controller.LoginControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminControllerServlet</servlet-name>
    <servlet-class>evoting.controller.AdminControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCandidateControllerServlet</servlet-name>
    <servlet-class>evoting.controller.AddCandidateControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationControllerServlet</servlet-name>
    <url-pattern>/RegistrationControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginControllerServlet</servlet-name>
    <url-pattern>/LoginControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminControllerServlet</servlet-name>
    <url-pattern>/AdminControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddCandidateControllerServlet</servlet-name>
    <url-pattern>/AddCandidateControllerServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <servlet>
    <display-name>AddNewCandidateControllerServlet</display-name>
    <servlet-name>AddNewCandidateControllerServlet</servlet-name>
    <servlet-class>evoting.controller.AddNewCandidateControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddNewCandidateControllerServlet</servlet-name>
    <url-pattern>/AddNewCandidateControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>VotingControllerServlet</display-name>
    <servlet-name>VotingControllerServlet</servlet-name>
    <servlet-class>evoting.controller.VotingControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VotingControllerServlet</servlet-name>
    <url-pattern>/VotingControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddVoteControllerServlet</display-name>
    <servlet-name>AddVoteControllerServlet</servlet-name>
    <servlet-class>evoting.controller.AddVoteControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddVoteControllerServlet</servlet-name>
    <url-pattern>/AddVoteControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ElectionResultControllerServlet</display-name>
    <servlet-name>ElectionResultControllerServlet</servlet-name>
    <servlet-class>evoting.controller.ElectionResultControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ElectionResultControllerServlet</servlet-name>
    <url-pattern>/ElectionResultControllerServlet</url-pattern>
  </servlet-mapping>
</web-app>